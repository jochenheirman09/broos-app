{
  "hosting": [
    {
      "target": "broos-2-0",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "broos-2-0",
      "source": ".",
      "frameworksBackend": {
        "secrets": [
          "GEMINI_API_KEY",
          "FIREBASE_ADMIN_SERVICE_ACCOUNT"
        ]
      }
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}